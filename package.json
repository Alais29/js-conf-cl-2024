{
  "name": "@uxcorp/js-conf-chile-2024",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "turbo run dev",
    "build": "turbo run build",
    "build:apps": "turbo run build --filter=@uxcorp/jsconf --filter=@uxcorp/previa",
    "dev:jsconf": "turbo run dev --filter=@uxcorp/jsconf",
    "build:jsconf": "turbo run build --filter=@uxcorp/jsconf",
    "dev:previa": "turbo run dev --filter=@uxcorp/previa",
    "build:previa": "turbo run build --filter=@uxcorp/previa",
    "ui": "pnpm --filter @uxcorp/ui",
    "clean": "git clean -xdf node_modules",
    "clean:workspaces": "turbo clean"
  },
  "devDependencies": {
    "turbo": "^2.1.3"
  },
  "engines": {
    "node": ">=20.14.0",
    "pnpm": "^9.10.0"
  },
  "packageManager": "pnpm@9.10.0"
}
