---
import Communities from '@sections/Communities.astro';
import Conf from '@sections/Conf.astro';
import Event from '@sections/Event.astro';
import Hero from '@sections/Hero.astro';
import Speakers from '@sections/Speakers.astro';
import Sponsors from '@sections/Sponsors.astro';
import Staff from '@sections/Staff/Staff.astro';
