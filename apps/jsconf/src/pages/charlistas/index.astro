---
// WorkshopsTab
import { HeroSpeakers, SpeakersTab } from '@sections';
import { TabsScheduleCharlistas } from '@components';
import Layout from '@layouts/Layout.astro';
---

<Layout
  title="JSConf Chile 2024 | Charlistas"
  description="Conoce a los charlistas de JSConf Chile 2024. Descubre a los expertos en JavaScript y desarrollo web que estarán compartiendo sus conocimientos del 5 al 7 de diciembre en Santiago. ¡Explora sus perfiles y temas!"
  ogImg="home.png"
>
  <HeroSpeakers />
  <TabsScheduleCharlistas
    data={[
      {
        status: 'open',
        Content: SpeakersTab,
        triggerTitle: 'Charlistas'
      }
      // {
      //   status: 'closed',
      //   Content: WorkshopsTab,
      //   triggerTitle: 'Workshops'
      // }
    ]}
  />
</Layout>
